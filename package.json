  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "clean-files": "rm -rf template",
    "copy-files": "mkdir template && cp -a ./src/. template/src && cp -a ./public/. template/public &&  cp -a ./cypress/. template/cypress && cp .eslintignore .eslintrc.js .prettierrc.js .npmrc craco.config.js cypress.json gitignore README.md template/",
    "prepublishOnly": "yarn clean-files && yarn copy-files",
    "commit": "npx git-cz",

    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "fix": "eslint src --ext .js,.jsx,.ts,.tsx  --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "e2e": "cypress run --spec 'cypress/integration/*'"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios-mock-adapter": "^1.19.0",
    "cypress": "^7.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.2",
    "json-server": "^0.16.3",
    "mocha": "^9.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "prettier": "^2.3.2",
    "source-map-explorer": "^2.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
